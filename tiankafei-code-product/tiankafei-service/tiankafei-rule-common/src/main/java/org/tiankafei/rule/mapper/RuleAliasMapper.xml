<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.tiankafei.rule.mapper.RuleAliasMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="baseResultMap" type="org.tiankafei.rule.entity.RuleAliasEntity">
        <id column="id" property="id" />
        <result column="cur_dataset_unique_identifier" property="curDatasetUniqueIdentifier" />
        <result column="step_dataset_unique_identifier" property="stepDatasetUniqueIdentifier" />
        <result column="alias_name" property="aliasName" />
        <result column="version" property="version" />
        <result column="delete_mark" property="deleteMark" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="create_user_id" property="createUserId" />
        <result column="update_user_id" property="updateUserId" />
    </resultMap>


    <!-- 通用查询映射结果 -->
    <resultMap id="voResultMap" type="org.tiankafei.rule.vo.RuleAliasVo">
        <id column="id" property="id" />
        <result column="cur_dataset_unique_identifier" property="curDatasetUniqueIdentifier" />
        <result column="step_dataset_unique_identifier" property="stepDatasetUniqueIdentifier" />
        <result column="alias_name" property="aliasName" />
        <result column="version" property="version" />
        <result column="delete_mark" property="deleteMark" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="create_user_id" property="createUserId" />
        <result column="update_user_id" property="updateUserId" />
    </resultMap>


    <!-- 通用查询结果列 -->
    <sql id="base_Column_List">
        id, cur_dataset_unique_identifier, step_dataset_unique_identifier, alias_name, version, delete_mark, create_time, update_time, create_user_id, update_user_id
    </sql>
	
    <!-- 校验 规则的别名 是否已经存在 -->
    <select id="checkRuleAliasServiceExists" resultType="java.lang.Boolean">
        
    </select>
	
	<!-- 保存 规则的别名 -->
    <select id="addRuleAliasService" resultType="java.lang.Object">
        
    </select>

	<!-- 批量保存 规则的别名 -->
    <select id="batchAddRuleAliasService" resultType="java.lang.Object">
        
    </select>
	
	<!-- 删除 规则的别名 -->
    <select id="deleteRuleAliasService" resultType="java.lang.Boolean">
        
    </select>
	
	<!-- 批量删除 规则的别名 -->
    <select id="batchDeleteRuleAliasService" resultType="java.lang.Boolean">
        
    </select>
	
	<!-- 根据条件删除 规则的别名 -->
    <select id="conditionDeleteRuleAliasService" resultType="java.lang.Boolean">
        
    </select>
	
	<!-- 修改 规则的别名 -->
    <select id="updateRuleAliasService" resultType="java.lang.Boolean">
        
    </select>
	
	<!-- 根据ID获取 规则的别名 对象 -->
    <select id="getRuleAliasServiceById" resultMap="voResultMap">
        
    </select>
	
	<!-- 获取 规则的别名 对象列表 -->
    <select id="getRuleAliasServiceList" resultMap="voResultMap">
        select
            <include refid="base_Column_List"/>
        from sys_rule_alias
        <where>
            <if test="param.idList != null and param.idList.size != 0">
                and id in
                <foreach collection="param.idList" item="id" separator="," index="index" open="(" close=")">
                    #{id}
                </foreach>
            </if>
            <if test="param.deleteMark != null">
                and delete_mark = #{param.deleteMark}
            </if>
        </where>
    </select>
	
	<!-- 获取 规则的别名 分页对象列表 -->
    <select id="getRuleAliasServicePageList" resultMap="voResultMap">
        select
            id
        from sys_rule_alias
        <where>
            <if test="param.deleteMark != null">
                and delete_mark = #{param.deleteMark}
            </if>
        </where>
    </select>
	
	<!-- 计算 规则的别名 总记录数 -->
    <select id="countRuleAliasService" resultType="java.lang.Integer">
        
    </select>

</mapper>
